{
  "mcpServers": {
    "voyage-ai-search": {
      "command": "python",
      "args": [
        "voyage-search/src/mcp_server.py"
      ],
      "env": {
        "VOYAGE_API_KEY": "${VOYAGE_API_KEY}"
      },
      "description": "Enhanced semantic code search with Voyage AI embeddings",
      "features": [
        "Contextualized embeddings with voyage-context-3",
        "Binary rescoring for performance",
        "Voyage AI reranking with rerank-2.5-lite",
        "Hybrid search (semantic + lexical)",
        "Multiple analysis types (explain, optimize, review, refactor)"
      ]
    }
  }
}